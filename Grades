#include<stdio.h>
int main()
{
	int a,b,c,d,e,p;
	printf("enter subjectb marks");
	scanf("%d %d %d %d %d",&a,&b,&c,&d,&e);
	p=(a+b+c+d+e)/5;
	if (per>=90)
		printf("grade A");
	else if(per<90 &&per>=80)
		printf("grade B");
	else if(per<80 &&per>=70)
		printf("grade c");
	else if(per<70 &&per>=60)
		printf("grade D");
	else if(per<60 &&per>=40)
		printf("grade E");
	else
		printf("grade F")
		
}
	
	
	
